{
  "name": "mdncomp",
  "version": "2.4.0",
  "description": "MDN Browser Compatibility Data (BCD) on the command line.",
  "bin": {
    "mdncomp": "./bin/mdncomp.js",
    "bcd": "./bin/mdncomp.js"
  },
  "author": {
    "name": "epistemex",
    "email": "epistemex@users.noreply.github.com"
  },
  "scripts": {
    "debug": "node ./src/mdncomp.js",
    "start": "node ./bin/mdncomp.js",
    "push": "parallelshell \"git push gitlab\" \"git push origin master\"",
    "pushf": "parallelshell \"git push -f gitlab\" \"git push -f origin master\"",
    "postinstall": "echo To update data run: [sudo] mdncomp --update."
  },
  "keywords": [
    "mdn",
    "developer",
    "docs",
    "browser",
    "compatibility",
    "data",
    "cli",
    "terminal",
    "bcd",
    "specs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/epistemex/mdncomp.git"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "bin/",
    "build/",
    "locale/en.json",
    "locale/es.json",
    "locale/no.json",
    "data/data.json",
    "data/data.md5",
    "wiki/",
    "package.json",
    "README.md",
    "LICENSE.txt",
    "Change.log"
  ],
  "dependencies": {},
  "devDependencies": {
    "grunt": "^1.0.3",
    "grunt-contrib-uglify-es": "^3.3.0"
  }
}
